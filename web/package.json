{
  "name": "@plantdex/web",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "rm -rf dist && webpack --mode=production",
    "gql": "npx apollo client:codegen --target=typescript --addTypename --endpoint=http://localhost:4040/plants --outputFlat=src/plant/interfaces",
    "ts": "tsc",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "format": "prettier --write --check \"!.cache\" \"!dist\" .",
    "pwa-assets": "pwa-asset-generator src/assets/images/original/plant.svg src/assets/images -t png"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "apollo3-cache-persist": "^0.9.1",
    "graphql": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^4.41.25",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-graphql-tag": "^3.1.0",
    "compression-webpack-plugin": "^7.1.1",
    "copy-webpack-plugin": "^7.0.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "html-webpack-plugin": "^5.0.0-alpha.10",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pwa-asset-generator": "^4.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
